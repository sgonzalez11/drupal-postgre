{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}
<div class="container-fluid">

    {% set entitystatus = form.entity_status.widget['#value'].0 %}
    {% set entitystatusoptions = form.entity_status.widget['#options'] %}
    {% set entityId = form['#entityId'] %}
    {#{ dsm(entitystatus) }#}
    {#{ kint(form.entity_status.widget['#value']) }#}
    {#{ dsm(_context|keys) }#}

    <div class="row">
        <div class="col-sm-6">{{ form.name }}</div>
        <div class="col-sm-3">{{ form.code }}</div>
        <div class="col-sm-3">{{ form.behaviour_as }}</div>
    </div>
    
    <div class="row">
        <div class="col-sm-12">{{ form.actions }}</div>
    </div>
    {# Don't forget to add printing form special info at the end.
    Without this data form will not working propertly.
    #}
    {{ form.form_build_id }}
    {{ form.form_token }}
    {{ form.form_id }}
</div>
